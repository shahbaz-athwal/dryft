{
  "name": "hono-api",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun with-env --watch src/server.ts",
    "agent": "bun run src/agents.ts",
    "agent:dev": "bun with-env --watch src/agents.ts",
    "start": "bun run src/server.ts",
    "clean": "rm -rf node_modules",
    "db:generate": "bun with-env prisma generate",
    "db:migrate": "bun with-env prisma migrate dev",
    "db:studio": "bun with-env prisma studio",
    "db:push": "prisma db push --skip-generate",
    "with-env": "bun --env-file ../../.env"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.43",
    "@better-upload/server": "^3.0.7",
    "@browserbasehq/sdk": "^2.6.0",
    "@browserbasehq/stagehand": "^3.0.7",
    "@inngest/agent-kit": "^0.13.2",
    "@orpc/server": "^1.13.1",
    "@posthog/ai": "^7.3.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "ai": "^5.0.116",
    "axios": "^1.13.2",
    "better-auth": "^1.4.9",
    "graphql-request": "^7.4.0",
    "hono": "^4.11.3",
    "inngest": "^3.48.1",
    "posthog-node": "^5.18.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "prisma": "^7.2.0",
    "prisma-json-types-generator": "^4.0.1",
    "typescript": "^5.9.3"
  },
  "exports": {
    "./rpc": "./src/routes/rpc.ts"
  }
}
