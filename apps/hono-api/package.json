{
  "name": "hono-api",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun with-env --watch src/server.ts",
    "agent": "bun run src/agents.ts",
    "start": "bun run src/server.ts",
    "clean": "rm -rf node_modules",
    "db:generate": "bun with-env prisma generate",
    "db:migrate": "bun with-env prisma migrate dev",
    "db:studio": "bun with-env prisma studio",
    "db:push": "prisma db push --skip-generate",
    "with-env": "bun --env-file ../../.env"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.23",
    "@inngest/agent-kit": "^0.13.2",
    "@orpc/server": "^1.9.3",
    "@prisma/client": "^6.17.1",
    "ai": "^5.0.78",
    "axios": "^1.10.0",
    "better-auth": "catalog:",
    "graphql-request": "^7.2.0",
    "hono": "^4.10.2",
    "inngest": "^3.44.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "prisma": "^6.17.1",
    "typescript": "catalog:"
  },
  "exports": {
    "./rpc": "./src/routes/rpc.ts"
  }
}
